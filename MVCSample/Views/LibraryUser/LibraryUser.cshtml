@model  IEnumerable<UserEntity.User>

@{
    ViewBag.Title = "LibraryUser";
}

<h2>LibraryUser</h2>

<table>
    <tr>
        <td>LibraryUser</td>
       
    </tr>
  
            @foreach(UserEntity.User user in (List<UserEntity.User>)ViewData["user"])
            {
                <tr>
                    <td>
                        @user.userName
                    </td>
                    <td>
                        @user.name;
                    </td>
                </tr>

            }
    @foreach(UserEntity.User user in (IEnumerable<UserEntity.User>)TempData["user"])
    {
        <tr>
            <td>
                @user.userName;
            </td>
            <td>
                @user.email;
            </td>
        </tr>
    }


</table>

